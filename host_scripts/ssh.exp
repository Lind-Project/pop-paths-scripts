#!/usr/bin/expect

# logging into ssh

set index [lindex $argv 0];

spawn bash ssh_command.sh $index
expect {
    "yes/no" {
        send "yes\r"
        expect {
            "password" {
                send "password\r"
            }
        }
    } 
    
    "password" {
        send "password\r"
    }

}

interact
